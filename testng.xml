<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Womaty Regression Suite" parallel="false">

    <test name="Regression Tests">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>

        <packages>
            <package name="tests"/>
        </packages>

        <classes>
            <class name="tests.LoginTest"/>
            <class name="tests.MyIdentityTest"/>
            <class name="tests.MyPreferencesTest"/>
            <class name="tests.WomantyMsgTest"/>
        </classes>
    </test>

</suite>
